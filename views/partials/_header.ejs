<h1>Header</h1>
<ul>
    <li>
        <a href="/">Home</a>
        
    </li>
    <% if(!locals.user){ %> 
    <li>
        <a href="/users/sign-in">Sign In</a>
    </li>

    <li>
        <a href="/users/sign-up">Sign Up</a>
    </li>
    <% }else{ %> 

    <li>
        <a href="/users/profile/?user_id=<%= locals.user._id %>">Profile</a>
        <a href="/users/sign-out">Sign Out</a>
    </li>
    <% } %> 
</ul>
